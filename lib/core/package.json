{
  "name": "@kombu/core",
  "version": "0.0.1",
  "author": "dangreco <dan.greco@live.com>",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "umd:main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.modern.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "check": "pnpm run build && check-export-map",
    "clean": "shx rm -rf ./node_modules",
    "lint": "eslint src/**/*",
    "format": "prettier --write src/**/*",
    "build": "microbundle"
  },
  "eslintConfig": {
    "extends": "@kombu/eslint-config"
  },
  "prettier": "@kombu/prettier-config",
  "devDependencies": {
    "@kombu/eslint-config": "workspace:^",
    "@kombu/prettier-config": "workspace:^",
    "@types/node": "^20.4.2",
    "check-export-map": "^1.3.0",
    "custom-card-helpers": "^1.9.0",
    "eslint": "^8.45.0",
    "home-assistant-js-websocket": "^8.2.0",
    "microbundle": "^0.15.1",
    "preact": "^10.16.0",
    "prettier": "^3.0.0",
    "shx": "^0.3.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "custom-card-helpers": ">=1.9.0",
    "preact": ">=10"
  },
  "dependencies": {
    "jotai": "^2.2.2"
  }
}
