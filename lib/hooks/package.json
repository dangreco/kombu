{
  "name": "@kombu/hooks",
  "version": "1.0.1",
  "author": "dangreco <dan.greco@live.com>",
  "homepage": "https://github.com/dangreco/kombu#readme",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "umd:main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.modern.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "check": "pnpm run build && check-export-map",
    "clean": "shx rm -rf ./dist",
    "lint": "eslint src/**/*",
    "format": "prettier --write src/**/*",
    "build": "microbundle"
  },
  "eslintConfig": {
    "extends": "@kombu/eslint-config"
  },
  "prettier": "@kombu/prettier-config",
  "devDependencies": {
    "@kombu/core": "workspace:^",
    "@kombu/eslint-config": "workspace:^",
    "@kombu/prettier-config": "workspace:^",
    "@types/react": "^18.2.17",
    "check-export-map": "^1.3.0",
    "custom-card-helpers": "^1.9.0",
    "eslint": "^8.45.0",
    "home-assistant-js-websocket": "^8.2.0",
    "jotai": "^2.2.2",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "shx": "^0.3.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@kombu/core": "workspace:^",
    "custom-card-helpers": "^1.9.0",
    "home-assistant-js-websocket": "^8.2.0",
    "jotai": "^2.2.2",
    "react": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangreco/kombu.git"
  },
  "bugs": {
    "url": "https://github.com/dangreco/kombu/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
